#include<stdio.h>
int main()
{
    printf(" Paneer Dosa , Rs- 150/-\n Veg Sandwich, Rs:-100/-\n Pizza , Rs:-250/-\n Burger, Rs:-100/- \n French fries, Rs:-200/-\n");
    int choice;
printf("Enter your choice : \n");
scanf("%d",&choice);
 
switch (choice)
{
    case 1: printf("Paneer Dosa , Rs:-150/-");
    break;
    case 2: printf("Veg Sandwich, Rs:-100/-");
    break;
    case 3: printf("Pizza , Rs:-250/-");
    break;
    case 4: printf("Burger, Rs:-100/-");
    break;
    case 5: printf("French fries, Rs:-200/-");
    break;
default:printf("Not in the menu");
    break;
}
return 0;

}
